      "targets": [
        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${DS_POSTGRESQL_BO}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT row_number() OVER(ORDER BY tdio.id)  nr_riga, \r\n   tdio.creation_date AS IL_data_creaz,\r\n    tdio.update_date AS IL_data_agg,\r\n    tdio.file_name AS IL_nome_file,\r\n    tdio.number_line AS IL_num_riga,\r\n    tdio.network_code AS IL_negozio,\r\n    tdio.processing_step1 AS IL_fase_1,\r\n    tdio.processing_step2 AS IL_fase_2,\r\n    tdio.processing_step3 AS IL_fase_3,\r\n   --- tdio.processing_step4 AS IL_fase_4,\r\n\ttdio.third_party as IL_forn,\r\n\ttdio.description as IL_descforn,\r\n\ttdio.business_name as IL_desclunga,\r\n    tdio.vat_number as IL_IVANUMB,\r\n    tdio.operational_agreement as IL_Accordo,\r\n    tdio.description_agreement as IL_DescAccordo, \r\n\t'* TAB_FORN *' as TAB_FORN,\r\n    ---tas.operational_agreement_id AS id_acc_oper_ges,\r\n    ttp.third_party as forn,\r\n    ttp.business_name as desc_forn,\r\n    tma.operational_agreement AS acc_oper,\r\n    tma.description AS desc_acc_oper,\r\n    '* DATI_INTERFACCIA *' as DATI_INTERFACCIA,\r\n    tdio.error_message AS IL_mess_err,\r\n    tdio.buffer_str as IL_buffer_dati\r\nFROM\r\n    tin_data_third_party_in tdio\r\nLEFT JOIN\r\n    tmd_networks tn ON tn.network_code = tdio.network_code\r\nLEFT JOIN\r\n    tmd_operational_agreements tma ON tma.operational_agreement = tdio.operational_agreement\r\nLEFT JOIN\r\n    tmd_third_parties ttp ON ttp.third_party = tdio.third_party\r\nWHERE\r\n    tdio.creation_date BETWEEN $__timeFrom() AND $__timeTo()\r\n----    AND (tdio.network_code IS NULL OR tdio.network_code IN ($vg_store))\r\n    AND tdio.processing_step1::text IN ($vg_fase1)\r\n    AND tdio.processing_step2::text IN ($vg_fase2)\r\n    AND tdio.processing_step3::text IN ($vg_fase3)\r\n   ----- AND tdio.processing_step4::text IN ($vg_fase4)\r\n AND (tdio.network_code IS NULL OR tdio.network_code IN ($vg_store))\r\n    order by   tdio.file_name ,\r\n    tdio.number_line ;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
