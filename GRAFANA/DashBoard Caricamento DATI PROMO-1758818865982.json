        {
          "datasource": {
            "type": "grafana-postgresql-datasource",
            "uid": "${DS_POSTGRESQL_BO}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT row_number() OVER(ORDER BY tdio.id)  nr_riga, \r\n   tdio.creation_date AS IL_data_creaz,\r\n    tdio.update_date AS IL_data_agg,\r\n    tdio.file_name AS IL_nome_file,\r\n    tdio.number_line AS IL_num_riga,\r\n    tdio.network AS IL_negozio,\r\n    tdio.processing_step1 AS IL_fase_1,\r\n    tdio.processing_step2 AS IL_fase_2,\r\n    tdio.processing_step3 AS IL_fase_3,\r\n    tdio.processing_step4 AS IL_fase_4,\r\n    tdio.promo_code as IL_Cod_promo,\r\n    tdio.promo_description as IL_desc_promo,\r\n    tdio.item AS IL_articolo,\r\n    tdio.sale_price as IL_prz_vend,\r\n    tdio.start_date_sale as IL_data_inizio,\r\n    tdio.end_date_sale as IL_data_fine,\r\n\ttdio.promo_type as IL_TIPO_PROMO ,\r\n\t'* TAB_PROMO *' as TAB_PROMO,\r\n\t'xxx', \r\n    '* DATI_INTERFACCIA *' as DATI_INTERFACCIA,\r\n    tdio.error_message AS IL_mess_err,\r\n    tdio.buffer_str as IL_buffer_dati\r\n from tin_Data_promotion_in tdio\r\n/********LEFT JOIN\r\n    tmd_networks tn ON tn.network_code = tdio.network\r\nLEFT JOIN\r\n    tmd_items ti ON ti.item = tdio.item\r\nLEFT JOIN\r\n    ttr_items ttr ON ttr.item_id = ti.id AND ttr.language_id = 1\r\nLEFT JOIN\r\n    tmd_item_sales tis ON tis.item_id = ti.id AND tis.item_sale = 1\r\n--LEFT JOIN\r\n --   vmd_items_structures vmd ON vmd.item_id = ti.id\r\n --   AND vmd.merchandise_structure_id = 1\r\n  --  AND vmd.structure_level = 4\r\nLEFT JOIN\r\n    tmd_orderable_assortments tas ON tas.item_id = ti.id\r\n    AND tas.network_id = tn.id\r\n    AND tas.main_supplier = 1\r\n    AND CURRENT_DATE BETWEEN tas.start_date AND tas.end_date\r\nLEFT JOIN\r\n    tmd_operational_agreements tma ON tma.id = tas.operational_agreement_id\r\nLEFT JOIN\r\n    tmd_third_parties ttp ON ttp.id = tma.third_party_id\r\nLEFT JOIN\r\n    tmd_purchase_prices tpp ON tpp.item_id = ti.id\r\n    AND tpp.operational_agreement_id = tas.operational_agreement_id\r\n    AND tpp.network_id = tn.id\r\n    AND CURRENT_DATE BETWEEN tpp.start_date AND tpp.end_date\r\nLEFT JOIN\r\n    tmd_sale_prices tsp ON tsp.network_id = tn.id\r\n    AND tsp.item_sale_id = tis.id\r\n    AND CURRENT_DATE BETWEEN tsp.start_date AND tsp.end_date*/\r\nWHERE\r\n    tdio.creation_date BETWEEN $__timeFrom() AND $__timeTo()\r\n    AND (tdio.network IS NULL OR tdio.network IN ($vg_store))\r\n    AND tdio.processing_step1::text IN ($vg_fase1)\r\n    AND tdio.processing_step2::text IN ($vg_fase2)\r\n ----   AND tdio.processing_step3::text IN ($vg_fase3)\r\n    AND tdio.processing_step4::text IN ($vg_fase4)\r\nAND (\r\n  '$vg_codart' = '' \r\n  OR tdio.item like '$vg_codart' collate case_like\r\n)\r\nAND (\r\n  '$vg_nomefile' = '' \r\n  OR tdio.file_name like '$vg_nomefile' collate case_like\r\n)\r\n    order by   tdio.file_name ,\r\n    tdio.number_line ;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
