          "rawSql": "SELECT     count(*) AS TOT_righe, \r\n   SUM( CASE WHEN processing_step1 <>0 THEN 1 END) AS FASE1_ELAB ,\r\n   SUM( CASE WHEN processing_step1 <>0 and processing_step2 <>0  THEN 1 END) AS FASE2_ELAB,\r\n   SUM( CASE WHEN processing_step1 <>0 and processing_step2 <>0  and processing_step3 <>0 THEN 1 END) AS FASE3_ELAB,\r\n   SUM( CASE WHEN processing_step1 <>0 and processing_step2 <>0  and processing_step3 <>0 and processing_step4 <>0THEN 1 END) AS FASE4_ELAB,\r\n   count(distinct promo_code) AS TOT_PROMO, \r\n   count(distinct CASE WHEN processing_step1 <>0 THEN promo_code END) AS FASE1_ELAB_PROMO ,\r\n   count(distinct CASE WHEN processing_step1 <>0 and processing_step2 <>0  THEN promo_code END) AS FASE2_ELAB_PROMO,\r\n   count(distinct CASE WHEN processing_step1 <>0 and processing_step2 <>0  and processing_step3 <>0 THEN promo_code END) AS FASE3_ELAB_PROMO,\r\n   count(distinct CASE WHEN processing_step1 <>0 and processing_step2 <>0  and processing_step3 <>0 and processing_step4 <>0THEN promo_code END) AS FASE4_ELAB_PROMO\r\nFROM tin_Data_promotion_in\r\nWHERE creation_date BETWEEN $__timeFrom() AND $__timeTo()\r\n  ----AND file_name LIKE '%IH%' COLLATE CASE_LIKE\r\n  AND (network IS NULL OR network IN ($vg_store));\r\n",
